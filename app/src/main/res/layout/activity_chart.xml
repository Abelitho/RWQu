<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hasilsuarapemilihan"
    tools:context=".HasilSuara">

    <com.github.mikephil.charting.charts.HorizontalBarChart
        android:id="@+id/chart1"
        android:layout_width="352dp"
        android:layout_height="45dp"
        android:layout_marginTop="152dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/percentage1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="12dp"
        android:text="28"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/chart1"
        app:layout_constraintStart_toEndOf="@+id/chart1"
        app:layout_constraintTop_toTopOf="@+id/chart1"
        app:layout_constraintVertical_bias="0.461" />

    <com.github.mikephil.charting.charts.HorizontalBarChart
        android:id="@+id/chart2"
        android:layout_width="352dp"
        android:layout_height="45dp"
        android:layout_marginTop="152dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="@+id/chart1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/chart1"
        app:layout_constraintTop_toBottomOf="@+id/chart1" />

    <TextView
        android:id="@+id/percentage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="12dp"
        android:text="28"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/chart2"
        app:layout_constraintStart_toEndOf="@+id/chart2"
        app:layout_constraintTop_toTopOf="@+id/chart2"
        app:layout_constraintVertical_bias="0.523" />

    <com.github.mikephil.charting.charts.HorizontalBarChart
        android:id="@+id/chart3"
        android:layout_width="352dp"
        android:layout_height="45dp"
        android:layout_marginTop="152dp"
        android:layout_weight="1"
        app:layout_constraintEnd_toEndOf="@+id/chart2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/chart2"
        app:layout_constraintTop_toBottomOf="@+id/chart2" />

    <TextView
        android:id="@+id/percentage3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="12dp"
        android:text="28"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toBottomOf="@+id/chart3"
        app:layout_constraintStart_toEndOf="@+id/chart3"
        app:layout_constraintTop_toTopOf="@+id/chart3"
        app:layout_constraintVertical_bias="0.473" />

    <Button
        android:id="@+id/btn_kembalihasilpemilihan2"
        android:layout_width="200sp"
        android:layout_height="60sp"
        android:backgroundTint="#8B1E2E"
        android:text="Kembali"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />
</androidx.constraintlayout.widget.ConstraintLayout>